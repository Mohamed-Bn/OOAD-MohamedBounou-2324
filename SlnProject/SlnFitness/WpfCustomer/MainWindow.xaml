<Window x:Class="WpfCustomer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfCustomer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Statistieken" HorizontalAlignment="Left" Margin="25,29,0,0" VerticalAlignment="Top" Height="24" Width="100" Click="StatistiekenButton_Click"/>
        <Button Content="Workouts" HorizontalAlignment="Left" Margin="25,72,0,0" VerticalAlignment="Top" Height="24" Width="100" Click="WorkoutsButton_Click"/>
        <Button Content="Uitloggen" HorizontalAlignment="Left" Margin="25,114,0,0" VerticalAlignment="Top" Height="24" Width="100" Click="logout"/>
        <Image x:Name="foto_abdel" HorizontalAlignment="Left" Height="100" Margin="25,279,0,0" VerticalAlignment="Top" Width="100" RenderTransformOrigin="0.12,0.91"/>
        <Frame x:Name="MainFrame" Margin="148,10,10,55" Navigated="MainFrame_Navigated"/>
    </Grid>
</Window>
